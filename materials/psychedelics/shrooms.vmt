"Refract"
{

	<dx90
	{
		 	"$fallbackmaterial" "psychedelics/shrooms_dx80"
	}

	"$refractamount" ".02"
	"$refracttint" "{255 255 255}"
	"$refractblur" "0"


	"$normalmap" "psychedelics/textures/mushroom"
	"$bumpframe" "0"


	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" .005
			"texturescrollangle" 45.00
		}
	}
}
